import java.util.*;
public class Main {
    public static void main (String args[]) {
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int cc=0;
int arr[]=new int[3];
while(n-->0)
{
    int c=0;
 int k=0;
 while(k<3)
 arr[k++]=sc.nextInt();
 for(int x:arr)
 {
     if(x==1)
     c++;
 }
 if(c>=2)
 cc++;
 
}
System.out.println(cc);
    }
}